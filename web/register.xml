<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="../ph.xsl"?>

<page>
  <title>Team Registration</title>
  <form>
    <input type="text" id="name">
      Team name
    </input>
    <input type="password" id="password">
      Password
    </input>
    <input type="password" id="password_verify">
      Repeat password
    </input>
    <input type="text" id="member_1">
      Team member (email address)
    </input>
    <input type="text" id="member_2">
      Team member
    </input>
    <input type="text" id="member_3">
      Team member
    </input>
    <input type="text" id="member_4">
      Team member
    </input>
    <submit-button action="register">
      Register Team
    </submit-button>
  </form>
</page>
