<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="../ph.xsl"?>

<page>
  <title>Submit an Answer</title>
  <form>
    <input type="text" id="name">
      Team name
    </input>
    <input type="password" id="password">
      Password
    </input>
    <input type="puzzle" id="puzzle">
      Puzzle
    </input>
    <input type="text" id="guess">
      Answer
    </input>
    <submit-button action="guess">
      Submit Answer
    </submit-button>
  </form>
</page>
