<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="/ph.xsl"?>

<page>
  <hunt key="{{hunt.key}}">{{hunt.name}}</hunt>
  <title>Puzzle Stats</title>
  <waves>
    {{#waves}}
    <wave name="{{name}}">
      <puzzles>
        {{#puzzles}}
        <puzzle-stats name="{{name}}" number="{{number}}" key="{{key}}"
                      guesses="{{guesses}}" solves="{{solves}}" avgSolveTime="{{avgSolveTime}}"/>
        {{/puzzles}}
      </puzzles>
    </wave>
    {{/waves}}
  </waves>
</page>
