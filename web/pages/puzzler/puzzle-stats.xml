<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="/ph.xsl"?>

<page>
  <hunt key="{{hunt.key}}">{{hunt.name}}</hunt>
  <title>{{puzzle.name}}</title>
  <prose>
    <table>
      <tr>
        <th>Team</th>
        <th>Score</th>
        <th>Solve Time</th>
        <th>Guesses</th>
      </tr>
      {{#stats}}
        <tr>
          <td>{{team}}</td>
          <td>{{score}}</td>
          <td>{{solveTime}}</td>
          <td>{{guesses}}</td>
        </tr>
      {{/stats}}
    </table>
  </prose>
</page>
