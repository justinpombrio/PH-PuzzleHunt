<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="../ph.xsl"?>

<master-page>
  <title>Hunt Info</title>
  <form>
    <input type="text" id="name">
      Name of the hunt
    </input>
    <input type="text" id="team-size">
      Maximum team size
    </input>
    <input type="text" id="init-guesses">
      How many guesses each team starts with
    </input>
    <submit-button action="SetHunt">
      Update
    </submit-button>
  </form>
</master-page>
