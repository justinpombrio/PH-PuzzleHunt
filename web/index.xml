<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="/ph.xsl"?>

<page page-type="global">
  <title>All Hunts</title>
  <prose>
    <table>
      {{#hunts}}
        {{#visible}}
        <p>
          <a href="{{key}}/index.xml">{{name}}</a>
          {{#closed}}(closed){{/closed}}{{^closed}}(open){{/closed}}
        </p>
        {{/visible}}
      {{/hunts}}
    </table>
  </prose>
</page>
